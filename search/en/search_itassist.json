---
title: search
layout: none
search: exclude
---


[
    {% assign pagesHtml = "" | split: ',' %}
{% for page in site.html_pages  %}
{% if  page.url contains "ko/platform"  or page.url contains "/index.html" or page.url contains "ko/smartassist" or page.url contains "ko/searchassist" or  page.url contains "ko/itassist" or  page.url contains "en/platform"  or page.url contains "en/smartassist" or page.url contains "en/searchassist"   %}
{% else %}
{% unless page.search == "exclude" %}
{% assign pagesHtml = pagesHtml | push: page %}



{% endunless %}
{% endif  %}
{% endfor %}

{% for page in pagesHtml  %}
{
    "title": "{{ page.title | escape }}",
    "tags": "{{ page.tags }}",
    "keywords": "{{page.keywords}}",
    "url": "/docs{{page.url}}",
    "summary": "{{page.summary | strip }}",
    "content": "{{page.content | strip_html | escape | newline_to_br  | strip_newlines | remove:'<br />' | remove: '#'}}"
    }
    {% unless forloop.last  %},{% endunless %}
    {% endfor %}

]
